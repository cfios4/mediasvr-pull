{
  "ignition": {
    "version": "3.4.0"
  },
  "passwd": {
    "groups": [
      {
        "name": "docker"
      }
    ],
    "users": [
      {
        "groups": [
          "wheel",
          "docker"
        ],
        "name": "casey",
        "sshAuthorizedKeys": [
          "ssh-rsa  casey@GamingPC"
        ]
      }
    ]
  },
  "storage": {
    "disks": [
      {
        "device": "/dev/sda",
        "partitions": [
          {
            "label": "0",
            "sizeMiB": 0,
            "startMiB": 0
          }
        ]
      }
    ],
    "files": [
      {
        "overwrite": true,
        "path": "/etc/hostname",
        "contents": {
          "compression": "",
          "source": "data:,coreos"
        },
        "mode": 420
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=etcd member\nDocumentation=https://github.com/coreos/etcd\n[Service]\nExecStart=/usr/bin/etcd\nRestart=always\n[Install]\nWantedBy=multi-user.target\n",
        "enabled": true,
        "name": "etcd-member.service"
      }
    ]
  }
}
